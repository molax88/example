   <select id="selectByUserId" parameterType="java.lang.Integer" resultMap="BaseResultMap">
	    SELECT
		  r.* 
		FROM
		  `auth_user_role` ur,
		  `auth_role_resource` rr,
		  `auth_resource` r 
		WHERE ur.`role_id` = rr.`role_id` 
		  AND rr.`resource_id` = r.`id` 
		  AND r.`status` = '1' 
		  AND ur.`user_id` = #{userId,jdbcType=INTEGER}
		  ORDER BY r.id,r.order_num;
  </select>
